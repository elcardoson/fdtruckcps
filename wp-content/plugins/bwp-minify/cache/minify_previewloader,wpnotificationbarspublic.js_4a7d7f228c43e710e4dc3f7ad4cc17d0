var googlePublisherPluginPreviewLoader={};googlePublisherPluginPreviewLoader.FRONTEND_URL='https://publisherplugin.google.com';googlePublisherPluginPreviewLoader.previewInjectorScriptLoaded=false;googlePublisherPluginPreviewLoader.receiveMessage=function(event){if(event.origin!==googlePublisherPluginPreviewLoader.FRONTEND_URL){return;}
var data=(JSON.parse(event.data));if(data.action=='load_script'&&data.relativeScriptSrc.indexOf('/_/publisher_plugin/')==0&&!googlePublisherPluginPreviewLoader.previewInjectorScriptLoaded){var script=document.createElement('script');script.type='text/javascript';script.src='https://www.gstatic.com'+
data.relativeScriptSrc;document.body.appendChild(script);googlePublisherPluginPreviewLoader.previewInjectorScriptLoaded=true;}};googlePublisherPluginPreviewLoader.getScripts=function(){if(window.self!==window.parent){window.top.frames['admin-iframe'].postMessage(JSON.stringify({action:'get_scripts'}),'https://publisherplugin.google.com');}};window.addEventListener('message',googlePublisherPluginPreviewLoader.receiveMessage,false);window.addEventListener('DOMContentLoaded',googlePublisherPluginPreviewLoader.getScripts,false);;(function($){'use strict';$(function(){var barHeight;if($('.mtsnb').length>0){barHeight=$('.mtsnb').outerHeight();$('body').css('padding-top',barHeight).addClass('has-mtsnb');}
$(document).on('click','.mtsnb-hide',function(e){e.preventDefault();var $this=$(this);if(!$this.hasClass('active')){$this.closest('.mtsnb').removeClass('mtsnb-shown').addClass('mtsnb-hidden');$('body').css('padding-top',0);}});$(document).on('click','.mtsnb-show',function(e){e.preventDefault();var $this=$(this);if(!$this.hasClass('active')){barHeight=$('.mtsnb').outerHeight();$this.closest('.mtsnb').removeClass('mtsnb-hidden').addClass('mtsnb-shown');$('body').css('padding-top',barHeight);}});});})(jQuery);